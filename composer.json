{
  "name": "mukurtu/mukurtu-cms",
  "description": "Mukurtu CMS Profile v4 Description",
  "type": "drupal-profile",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "mukurtu/masonry",
        "version": "4.2.2",
        "dist": {
          "url": "https://github.com/desandro/masonry/archive/v4.2.2.zip",
          "type": "zip"
        },
        "type": "drupal-library"
      }
    }
  ],
  "require": {
    "drupal/blazy": "^2.1",
    "drupal/core": "~8.8.8",
    "drupal/og": "*",
    "drupal/restui": "*",
    "drupal/search_api": "*",
    "drupal/config_sync": "^2.0@beta",
    "drupal/csv_serialization": "*",
    "mukurtu/masonry": "*",
    "drupal/embed": "*",
    "drupal/entity_embed": "*",
    "drupal/entity_browser": "*",
    "drupal/content_browser": "*",
    "drupal/diff": "*",
    "drupal/geolocation": "*",
    "drupal/geolocation_gpx": "*",
    "drupal/flag": "*",
    "drupal/field_group": "3.x-dev",
    "drupal/message": "*",
    "drupal/message_ui": "*",
    "drupal/message_digest": "*",
    "drupal/message_digest_ui": "*",
    "drupal/message_notify": "*",
    "drupal/message_notify_ui": "*",
    "drupal/message_subscribe": "*",
    "drupal/message_subscribe_email": "*",
    "drupal/message_subscribe_ui": "*",
    "drupal/entity_reference_revisions": "*",
    "drupal/paragraphs": "*",
    "drupal/slick": "^2.2",
    "drupal/slick_entityreference": "^2.0"
  },
  "require-dev": {
    "drupal/devel": "*",
    "drupal/devel_php": "*",
    "drupal/core-dev": "*",
    "drupal/coder": "^8.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
  }
}
