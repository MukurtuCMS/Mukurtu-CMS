{
  "name": "mukurtu/mukurtu-cms",
  "description": "Mukurtu CMS Profile v4 Description",
  "type": "drupal-profile",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
